<div class="survey_user">
  <h1><%= @survey.title %></h1>


    <%@survey.questions.each do |question| %>
      <li class="black"><p><%= question.question %></p></li>
            <li class="white"><p><%=current_user.choices.where(question_id: question.id).first.choice%><p></li>
    <%end%>


  <form action="/user/profile" method="get">
    <input class="button" type="submit" value="Back to Profile">
  </form>
</div>
